     }
    .form{
        margin: auto;
        background-color: white;
        width: 500px;
        height: 330px;
        text-align: center;
        margin-top: 100px;
        padding: 10px;
        border-radius: 4px;
        text-align: right;
    }
    
    .form input{
        width: 250px;
        padding: 12px 20px;
        margin: 8px ;
        border: 1px solid #ccc;
        border-radius: 4px;
        margin-right: 80px;
        background-color: #F5F5F5;
    }
    
    input[type=submit]{
        margin-right: 125px;
        background-color: #35858B;
        color: white;
        border: 0;
    }
    input[type=submit]:hover {
                background-color: #3A9399;
